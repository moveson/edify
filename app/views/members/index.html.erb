<div class="row">
  <div class="col-sm-6">
    <h1>Members</h1>
  </div>

  <div class="col-sm-6 text-end">
    <%= link_to new_import_job_path, class: 'btn btn-primary' do %>
      Fetch Member Data
    <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
    <tr>
      <th>Name</th>
      <th>Gender</th>
      <th>Age</th>
      <th>Phone</th>
    </tr>
    </thead>

    <tbody id="members">
    <%= render partial: "member", collection: @members %>
    </tbody>
  </table>
</div>
